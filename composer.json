{
    "name": "skilla/base-development-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Zambrano Delfa",
            "email": "sergio.zambrano@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Skilla\\BaseDevelopmentBundle\\": "Bundle/"
        },
        "classmap": [
            "app/AppKernel.php"
        ],
        "exclude-from-classmap": [
            "Bundle/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skilla\\BaseDevelopmentBundle\\Tests\\": "Bundle/Tests/"
        }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "symfony/http-kernel": "~2.8.12 || ~3.1.0",
        "symfony/framework-bundle": "~2.8.12 || ~3.1.0",
        "symfony/console": "~2.8.12 || ~3.1.0",
        "phpunit/phpunit": "~4.8.26 || ~5.6.0",
        "incenteev/composer-parameter-handler": "^2.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
